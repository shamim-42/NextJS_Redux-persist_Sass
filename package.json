{
  "name": "nextjs_with_redux-persist_boilerplate",
  "version": "0.0.1",
  "main": "server/index.js",
  "scripts": {
    "lint-staged": "lint-staged",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "test:ci": "jest --maxWorkers=2 --ci",
    "test": "jest --watch",
    "serve": "NODE_ENV=production node server/index.js",
    "clean": "rimraf node_modules/.cache .next",
    "lint": "standard --verbose | snazzy",
    "now-start": "yarn serve",
    "lint:fix": "standard --fix --verbose | snazzy",
    "export": "next build && next export",
    "prod_start": "SET NODE_ENV=production node server.js",
    "build_x": "SET NODE_ENV=production next build",
    "x": "nodemon -w server server/index.js",
    "compile:sass": "node-sass src/scss/main.scss ./public/static/css/styles.comp.css",
    "prefix:css": "postcss ./public/static/css/styles.comp.css --use=autoprefixer -o ./public/static/css/styles.prefix.css",
    "compress:css": "node-sass ./public/static/css/styles.prefix.css ./public/static/css/styles.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css",
    "watch:sass": "node-sass src/scss/main.scss ./public/static/css/styles.css -w"
  },
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "antd": "^3.24.3",
    "axios": "^0.19.0",
    "babel-plugin-import": "^1.12.2",
    "compression": "^1.7.1",
    "eslint-utils": "^1.4.3",
    "express": "^4.16.2",
    "file-loader": "^5.0.2",
    "helmet": "^3.20.0",
    "humps": "^2.0.1",
    "immutable": "^3.8.2",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.15",
    "next": "^11.1.0",
    "next-compose-plugins": "^2.2.0",
    "next-fonts": "^1.2.0",
    "next-images": "^1.2.0",
    "next-offline": "^5.0.0",
    "next-redux-wrapper": "^2.1.0",
    "next-routes": "^1.4.2",
    "node-sass": "^4.13.1",
    "null-loader": "^3.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.0",
    "react-moment": "^0.9.6",
    "react-redux": "^7.0.3",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.2.0",
    "styled-normalize": "^8.0.6",
    "url-loader": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^9.1.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.0.0",
    "babel-plugin-inline-dotenv": "^1.1.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "jest": "^24.7.1",
    "lint-staged": "^9.0.0",
    "nodemon": "^1.12.1",
    "pre-commit": "^1.2.2",
    "react-test-renderer": "^16.2.0",
    "redux-devtools-extension": "^2.13.8",
    "rimraf": "^3.0.0",
    "snazzy": "^8.0.0",
    "standard": "^13.0.1",
    "webpack-bundle-analyzer": "^3.6.1",
    "autoprefixer": "^9.6.1",
    "browserify": "^16.3.0",
    "node-sass": "^4.12.0",
    "postcss-cli": "^6.1.3",
    "regenerator-runtime": "^0.13.3",
    "npm-run-all": "^4.1.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 10 chrome version",
      "last 10 firefox version",
      "last 10 safari version"
    ]
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": [
      "git add"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "env": [
      "jest"
    ]
  },
  "engines": {
    "node": ">= 8.12"
  },
  "jest": {
    "modulePaths": [
      "./src",
      "./src/test"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect",
      "@testing-library/react/cleanup-after-each"
    ]
  },
  "repository": "",
  "bugs": "",
  "author": "Muhammad SH <shamim.cse.bd@gmail.com>",
  "license": "ISC",
  "description": "next.js and redux-persist-boilerplate webapp 0.0.1"
}
